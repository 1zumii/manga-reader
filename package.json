{
  "name": "manga-reader",
  "type": "module",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "vite --host",
    "dev:script": "ts-node --project tsconfig.script.json src/data/crawler.ts",
    "build": "vite build",
    "preview": "vite preview --port 7900",
    "build:script": "tsc --project tsconfig.script.json",
    "update-hook": "simple-git-hooks",
    "type-check": "tsc --project ./tsconfig.json --skipLibCheck",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./"
  },
  "dependencies": {
    "@solidjs/router": "^0.8.2",
    "solid-js": "^1.7.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.9",
    "axios": "^1.1.3",
    "chalk": "^4.1.2",
    "eslint": "^8.28.0",
    "eslint-plugin-solid": "^0.13.2",
    "fs-extra": "^10.1.0",
    "iconv-lite": "^0.6.3",
    "less": "^4.1.3",
    "node-html-parser": "^6.1.4",
    "opencc": "^1.1.3",
    "simple-git-hooks": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.0",
    "vite": "^3.0.9",
    "vite-plugin-solid": "^2.3.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run lint"
  }
}
